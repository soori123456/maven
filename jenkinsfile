pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
				echo "build is successful"
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
				echo "test is working"
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
				echo "deployment is failed"
            }
        }
    }
}
